<form action="/simpsons" method="post">
  <fieldset class="form-group">
    <label for="formGroupExampleInput">Character Name</label>
    <input type="text" class="form-control" id="formGroupExampleInput" placeholder="Character Name" name="name">
  </fieldset>
  <fieldset class="form-group">
    <label for="formGroupExampleInput2">Image</label>
    <input type="url" class="form-control" name="image" id="formGroupExampleInput2" placeholder="Image">
  </fieldset>
  <fieldset class="form-group">
  <button type="submit" class="btn btn-danger">Submit</button>
  </fieldset>
</form>
